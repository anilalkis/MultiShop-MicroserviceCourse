@model GetCargoCustomerByIdDto
@{
    ViewData["Title"] = "UserAdressInfo";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}

<section class="content container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="chart-box">
                <h4>Kategori Listesi</h4>
                <div class="bs-example" data-example-id="bordered-table">
                    <h3>Kullanıcı Profil Bilgisi</h3>
                    <div class="alert alert-info">Kullanıcı Id:@Model.userCustomerId</div>
                    <div class="alert alert-info">Kullanıcı Adı:@Model.name</div>
                    <div class="alert alert-info">Email:@Model.email</div>
                    <div class="alert alert-info">Telefon:@Model.phone</div>
                    <div class="alert alert-info">İlçe-Şehir:@Model.city</div>
                    <div class="alert alert-info">Adres:@Model.address</div>
                </div>
            </div>
        </div>
    </div>
</section>

